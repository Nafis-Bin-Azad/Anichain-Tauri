{
  "$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/dev/tooling/cli/schema.json",
  "identifier": "com.anichain.dev",
  "productName": "AniChain",
  "version": "0.1.0",
  "app": {
    "windows": [
      {
        "label": "main",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "title": "AniChain"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all",
    "category": "DeveloperTool"
  }
}
